{
    "$schema": "./node_modules/wrangler/config-schema.json",
    "name": "nexflow-realtime",
    "main": "realtime/index.ts",
    "compatibility_date": "2024-06-01",
    "durable_objects":{
        "bindings": [{
            "name": "Chat",
            "class_name": "Chat"
        }]
    },
    "migrations":[{
        "tag":"v1",
        "new_sqlite_classes":["Chat"]
    }],
    "observability": {
    "logs": {
      "enabled": false,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  }

}

